BUILD=build
DIST=dist
CCFLAGS=-c -g -Wall -Wno-unused-value
LDFLAGS=-o $(DIST)/check_tor -lcurl 

all:	$(DIST)/check_tor
$(DIST)/check_tor:	$(BUILD) $(DIST) $(BUILD)/csocket.o $(BUILD)/misc.o $(BUILD)/checks.o $(BUILD)/prog.o 
	$(CC) $(BUILD)/*.o $(LDFLAGS)
$(BUILD)/csocket.o:	csocket.cpp csocket.h socketException.h
	$(CC) csocket.cpp $(CCFLAGS) -o $(BUILD)/csocket.o
$(BUILD)/misc.o:	misc.cpp misc.h config.h
	$(CC) misc.cpp $(CCFLAGS) -o $(BUILD)/misc.o
$(BUILD)/checks.o:	checks.cpp checks.h consts.h socketException.h csocket.h checkException.h misc.h
	$(CC) checks.cpp $(CCFLAGS) -o $(BUILD)/checks.o
$(BUILD)/prog.o:	prog.cpp checks.h consts.h misc.h
	$(CC) prog.cpp $(CCFLAGS) -o $(BUILD)/prog.o
$(BUILD):
	@mkdir $(BUILD)
$(DIST):
	@mkdir $(DIST)
clean:
	rm -rf $(BUILD) $(DIST)
install:
	install $(DIST)/check_tor /usr/local/bin/
