BUILD=build
DIST=dist
CCFLAGS=-c -g -Weverything -Wno-disabled-macro-expansion -Wno-padded -Wno-weak-vtables
LDFLAGS=-o $(DIST)/prog -lcurl 

all:	$(DIST)/prog
$(DIST)/prog:	$(BUILD) $(DIST) $(BUILD)/checks.o $(BUILD)/csocket.o $(BUILD)/misc.o $(BUILD)/prog.o 
	$(CC) $(BUILD)/*.o $(LDFLAGS)
$(BUILD)/checks.o:	checks.cpp checks.h consts.h socketException.h csocket.h checkException.h
	$(CC) checks.cpp $(CCFLAGS) -o $(BUILD)/checks.o
$(BUILD)/csocket.o:	csocket.cpp csocket.h socketException.h
	$(CC) csocket.cpp $(CCFLAGS) -o $(BUILD)/csocket.o
$(BUILD)/misc.o:	misc.cpp misc.h
	$(CC) misc.cpp $(CCFLAGS) -o $(BUILD)/misc.o
$(BUILD)/prog.o:	prog.cpp checks.h consts.h misc.h
	$(CC) prog.cpp $(CCFLAGS) -o $(BUILD)/prog.o
$(BUILD):
	@mkdir $(BUILD)
$(DIST):
	@mkdir $(DIST)
clean:
	rm -rf $(BUILD) $(DIST)
install:
	install $(DIST)/prog /usr/local/bin/
love:
